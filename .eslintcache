1rem\",\r\n          padding: \"7px 20px 7px 20px\",\r\n          backgroundColor: \"#16c79a\",\r\n          color: \"white\",\r\n          height: \"80%\",\r\n          fontSize: \"16px\",\r\n        }}\r\n        onClick\r\n      >\r\n        {label}\r\n      </button>\r\n    );\r\n  }\r\n  renderInput(name, label, type = \"text\") {\r\n    const { data, errors } = this.state;\r\n    return (\r\n      <Input\r\n        type={type}\r\n        name={name}\r\n        value={data[name]}\r\n        //label={label}\r\n        placeholder={label}\r\n        onChange={this.handleChange}\r\n        error={errors[name]}\r\n      />\r\n    );\r\n  }\r\n  renderInput1(name, label, value, type = \"text\") {\r\n    const { data, errors } = this.state;\r\n    return (\r\n      <AutoInput\r\n        type={type}\r\n        name={name}\r\n        value={value}\r\n        //label={label}\r\n        placeholder={label}\r\n        onChange={this.handleChange(value)}\r\n        error={errors[name]}\r\n      />\r\n    );\r\n  }\r\n  renderFile(name, label, value, type = \"file\") {\r\n    const { data, errors } = this.state;\r\n    return (\r\n      <AutoInput\r\n        type={type}\r\n        name={name}\r\n        value={value}\r\n        //label={label}\r\n        placeholder={label}\r\n        onChange={this.handleChange}\r\n        error={errors[name]}\r\n      />\r\n    );\r\n  }\r\n\r\n  renderTextField(name, label, type = \"text\") {\r\n    const { data, errors } = this.state;\r\n    return (\r\n      <TextField\r\n        type={type}\r\n        name={name}\r\n        value={data[name]}\r\n        label={label}\r\n        placeholder={label}\r\n        onChange={this.handleChange}\r\n        error={errors[name]}\r\n      />\r\n    );\r\n  }\r\n  renderDropDown1(\r\n    name,\r\n    label,\r\n    value1,\r\n    value2,\r\n    value3,\r\n    value4,\r\n    value5,\r\n    value6,\r\n    value7,\r\n    type = \"text\"\r\n  ) {\r\n    const { data, errors } = this.state;\r\n    retur